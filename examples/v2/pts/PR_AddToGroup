#!/usr/local/bin/perl

use blib;
use strict;
use warnings;

use AFS::PTS;

die "Usage: $0 uid gid\n" if $#ARGV != 1;

my $uid = shift;
my $gid = shift;

my $pts = AFS::PTS->new;
$AFS::CODE and print "AFS::CODE = $AFS::CODE\n";

my $ok = $pts->PR_AddToGroup($uid, $gid);
if ($AFS::CODE) { print "AFS::CODE = $AFS::CODE\n"; }
else            { print "success  = $ok\n"; }
